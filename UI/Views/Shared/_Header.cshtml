<script>
    $(document).ready(function () {
        $("#logOut").click(function () {
            localStorage.setItem("Username", "");
        });
        var fullName = "@User.Identity.Name".split("@@")[0];
        localStorage.setItem("Username", fullName);

        $("#loginUsername").text = localStorage.getItem("Username");
        $("#manasaNamzaga").css('cursor', 'pointer');
        $("#imgVision").css('cursor', 'default');
        $("#imglogo").css('cursor', 'pointer');
        $("#manasaNamzaga").click(function (event) {
            event.preventDefault();
            var url = '@Url.Action("Index", "Home")';
            window.location.href = url;
        });
    });
</script>
